{
  "expo": {
    "name": "my-pos",
    "slug": "my-pos",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./src/assets/icon/shop-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/icon/shop-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon/shop-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.my.pos",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/icon/shop-icon.png"
    }
  }
}